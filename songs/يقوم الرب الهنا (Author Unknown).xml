<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.3" modifiedIn="OpenLP 3.1.3" modifiedDate="2024-10-25T23:53:55">
  <properties>
    <titles>
      <title>يقوم الرب الهنا</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>يقوم الرب الهنا<br/>فيبيد أعدائنا<br/>الان يصعد لنصرنا</lines>
    </verse>
    <verse name="v2">
      <lines>شعبأ قويا يقيمنا<br/>فرحه علي رؤوسنا<br/>ندوس به ونسحق رأس عدونا</lines>
    </verse>
  </lyrics>
</song>
