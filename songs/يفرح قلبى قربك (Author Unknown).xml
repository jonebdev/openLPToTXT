<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.3" modifiedIn="OpenLP 3.1.3" modifiedDate="2024-10-25T23:53:55">
  <properties>
    <titles>
      <title>يفرح قلبى قربك</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>أشدو للملك وأهلل<br/>من مثلك الهى</lines>
    </verse>
    <verse name="v1b">
      <lines>يفرح قلبى قربك يملأ عينى مجدك<br/>أسجد لك وحدك سيدى أعبدك</lines>
    </verse>
  </lyrics>
</song>
