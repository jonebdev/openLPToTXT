<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.3" modifiedIn="OpenLP 3.1.3" modifiedDate="2024-10-25T23:53:48">
  <properties>
    <titles>
      <title>Lead Me Lord</title>
    </titles>
    <copyright>Public Domain</copyright>
    <authors>
      <author>Samuel Sebastian Wesley</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines>Lead me, Lord,<br/>Lead me in Thy righteousness;<br/>Make Thy way plain before my face.<br/>For it is Thou, Lord,<br/>Thou, Lord, only,<br/>That makest me dwell in safety.</lines>
    </verse>
  </lyrics>
</song>
