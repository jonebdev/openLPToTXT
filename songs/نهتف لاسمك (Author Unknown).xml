    <titles>
      <title>نهتف لاسمك</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>نهتف لأسمك نرفع مجدك نعلن برك يسوع يسوع</lines>
    </verse>
    <verse name="v2">
      <lines>نطلب وجهك حقق وعدك أعطى ملئك بالروح بالروح</lines>
    </verse>
  </lyrics>
</song>
