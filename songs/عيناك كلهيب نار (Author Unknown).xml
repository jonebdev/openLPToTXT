<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.3" modifiedIn="OpenLP 3.1.3" modifiedDate="2024-10-25T23:53:52">
  <properties>
    <titles>
      <title>عيناك كلهيب نار</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="i1">
      <lines>للجالس على العرش وللحمل<br/>إكرام<br/>للجالس على العرش وللحمل<br/>سلطان</lines>
    </verse>
    <verse name="c1">
      <lines>قدوس ..قدوس .. قدوس<br/>قدوس الرب</lines>
    </verse>
    <verse name="e1">
      <lines>عيناك كلهيب نار<br/>جمرات نار متقدة<br/>ونهره نار</lines>
    </verse>
  </lyrics>
</song>
