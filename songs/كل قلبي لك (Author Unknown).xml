<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.3" modifiedIn="OpenLP 3.1.3" modifiedDate="2024-10-25T23:53:53">
  <properties>
    <titles>
      <title>كل قلبي لك</title>
    </titles>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>كل قلبي لك كل حبي لك<br/>كل فكري وحياتي<br/>لك وحدك</lines>
    </verse>
    <verse name="v2">
      <lines>اخضع لك أحيا ملكك<br/>لك عمري يا يسوعي<br/>لك وحدك</lines>
    </verse>
  </lyrics>
</song>
